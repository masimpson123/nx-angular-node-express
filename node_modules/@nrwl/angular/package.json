{
  "name": "@nrwl/angular",
  "version": "13.8.1",
  "description": "Angular Plugin for Nx",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrwl/nx.git",
    "directory": "packages/angular"
  },
  "scripts": {
    "postinstall": "node ./scripts/nx-cli-warning.js"
  },
  "keywords": [
    "Monorepo",
    "Angular",
    "Jest",
    "Cypress",
    "CLI"
  ],
  "exports": {
    "./generators": "./generators.js",
    "./executors": "./executors.js",
    "./tailwind": "./tailwind.js",
    "./src/generators/utils": "./src/generators/utils/index.js",
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./nrwl-angular.d.ts",
      "esm2020": "./esm2020/nrwl-angular.mjs",
      "es2020": "./fesm2020/nrwl-angular.mjs",
      "es2015": "./fesm2015/nrwl-angular.mjs",
      "node": "./fesm2015/nrwl-angular.mjs",
      "default": "./fesm2020/nrwl-angular.mjs"
    },
    "./testing": {
      "types": "./testing/nrwl-angular-testing.d.ts",
      "esm2020": "./esm2020/testing/nrwl-angular-testing.mjs",
      "es2020": "./fesm2020/nrwl-angular-testing.mjs",
      "es2015": "./fesm2015/nrwl-angular-testing.mjs",
      "node": "./fesm2015/nrwl-angular-testing.mjs",
      "default": "./fesm2020/nrwl-angular-testing.mjs"
    }
  },
  "author": "Victor Savkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nrwl/nx/issues"
  },
  "homepage": "https://nx.dev",
  "builders": "./executors.json",
  "schematics": "./generators.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./migrations.json"
  },
  "dependencies": {
    "@angular-devkit/schematics": "~13.2.0",
    "@nrwl/cypress": "13.8.1",
    "@nrwl/devkit": "13.8.1",
    "@nrwl/jest": "13.8.1",
    "@nrwl/linter": "13.8.1",
    "@nrwl/storybook": "13.8.1",
    "@schematics/angular": "~13.2.0",
    "@phenomnomnominal/tsquery": "4.1.1",
    "ignore": "^5.0.4",
    "jasmine-marbles": "~0.8.4",
    "rxjs-for-await": "0.0.2",
    "webpack-merge": "5.7.3",
    "ts-node": "~9.1.1",
    "tsconfig-paths": "^3.9.0",
    "semver": "7.3.4",
    "tslib": "^2.3.0"
  },
  "module": "fesm2015/nrwl-angular.mjs",
  "es2020": "fesm2020/nrwl-angular.mjs",
  "esm2020": "esm2020/nrwl-angular.mjs",
  "fesm2020": "fesm2020/nrwl-angular.mjs",
  "fesm2015": "fesm2015/nrwl-angular.mjs",
  "typings": "nrwl-angular.d.ts",
  "sideEffects": false
}